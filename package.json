{
  "name": "faviator",
  "version": "1.2.0",
  "description": "A simple easy favicon generator.",
  "main": "lib/index.js",
  "scripts": {
    "preversion": "npm test && $EDITOR CHANGELOG.md",
    "version": "git add -A",
    "postpublish": "git push --all && git push --tags",
    "\n# TESTING SCRIPTS:": "",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "codecov",
    "lint:js": "eslint .",
    "\n# FORMATS:": "",
    "format": "prettier --write **/*.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -e $GIT_PARAMS"
    }
  },
  "dependencies": {
    "@faviator/create-svg-favicon": "^1.1.1",
    "@ycm.jason/logger": "^0.1.5",
    "@ycm.jason/svg-to-img": "^0.1.4",
    "commander": "^2.20.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@ycm.jason/eslint-config": "^1.3.1",
    "codecov": "^3.5.0",
    "eslint": "^6.1.0",
    "husky": "^3.0.1",
    "jest": "^24.8.0",
    "lint-staged": "^9.2.1",
    "prettier": "^1.18.2",
    "validate-commit": "^3.4.0"
  },
  "keywords": [
    "favicon",
    "generator",
    "generate",
    "icon"
  ],
  "author": "Jason Yu <me@ycmjason.com>",
  "license": "ISC",
  "lint-staged": {
    "*.js": ["npm run lint:js", "npm run format", "git add"]
  },
  "bin": {
    "faviator": "./bin/faviator.js"
  },
  "directories": {
    "example": "example",
    "lib": "lib",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/faviator/faviator.git"
  },
  "bugs": {
    "url": "https://github.com/faviator/faviator/issues"
  },
  "homepage": "https://github.com/faviator/faviator#readme"
}
